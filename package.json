{
  "name": "glory-cli",
  "version": "0.0.1",
  "description": "前端脚手架",
  "main": "index.js",
  "bin": {
    "glory-cli": "./bin/glory"
  },
  "scripts": {
    "release": "npm run release-patch && npm run write:npm:token && git push --follow-tags origin master && npm publish",
    "write:npm:token": "node script/write-npm-token.js",
    "release:first": "standard-version --first-release",
    "release-major": "standard-version --release-as major",
    "release-minor": "standard-version --release-as minor",
    "release-patch": "standard-version --release-as patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/novlan1/glory-cli.git"
  },
  "files": [
    "bin",
    "lib"
  ],
  "keywords": [
    "cli",
    "glory-cli",
    "脚手架"
  ],
  "author": "novlan1",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/novlan1/glory-cli/issues"
  },
  "homepage": "https://github.com/novlan1/glory-cli#readme",
  "dependencies": {
    "axios": "^0.26.1",
    "chalk": "^4.0.0",
    "commander": "^9.0.0",
    "download-git-repo": "^3.0.2",
    "figlet": "^1.5.2",
    "fs-extra": "^10.0.1",
    "inquirer": "^8.2.1",
    "log-symbols": "^4.1.0",
    "ora": "^5.0.0"
  },
  "devDependencies": {
    "standard-version": "^9.5.0"
  }
}